{
  "private": true,
  "name": "RemirrorApp",
  "version": "0.0.0",
  "scripts": {
    "android": "react-native run-android",
    "android:log": "react-native log-android",
    "ios": "react-native run-ios",
    "ios:log": "xcrun simctl spawn booted log stream --predicate 'processImagePath ENDSWITH \"RemirrorApp\" AND senderImagePath ENDSWITH \"RemirrorApp\"' --color always",
    "link": "react-native link",
    "pod:init": "cd ios && bundle exec pod init",
    "pod:install": "cd ios && bundle exec \"pod install --repo-update\"",
    "pod:update": "cd ios && bundle exec pod update",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "@types/react-navigation": "3.0.5",
    "react": "16.8.4",
    "react-native": "0.58.6",
    "react-native-gesture-handler": "1.1.0",
    "react-native-log-ios": "1.0.1",
    "react-native-webview": "5.3.0",
    "react-navigation": "3.3.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/react": "16.8.7",
    "@types/react-native": "0.57.38",
    "@types/react-test-renderer": "16.8.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.3.1",
    "jest": "24.3.1",
    "metro-config": "0.53.0",
    "metro-react-native-babel-preset": "0.53.0",
    "react-native-debugger-open": "0.3.18",
    "react-test-renderer": "16.6.3",
    "ts-jest": "24.0.0",
    "typescript": "3.3.3333"
  },
