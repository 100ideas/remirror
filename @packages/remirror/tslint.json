{
  "extends": "@kj/tslint-rules/react",
  "rules": {
    "custom-no-implicit-dependencies": [
      true,
      "dev",
      ["jest-axe", "jest", "jest-dom", "react-testing-library", "@test-utils", "@storybook"]
    ],
    "no-empty": false,
    "no-any": false,
    "no-object-literal-type-assertion": false,
    "no-console": false
  }
}
